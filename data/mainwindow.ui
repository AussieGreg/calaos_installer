padding-left: 0px;
padding-right: 10px;
min-height: 48px;
}

QFrame.FrameTitle
{
	color: white;
	font-weight: bold;
}

QFrame .HeaderButton
{
	color: #333;
	border: 2px solid #555;
	border-radius: 6px;
	padding: 4px;
	padding-left: 6px;
	padding-right: 6px;
	background: qradialgradient(cx: 0.3, cy: -0.4, fx: 0.3, fy: -0.4, radius: 1.35, stop: 0 #fff, stop: 1 #888);
	min-width: 30px;
	margin-top: 2px;
	margin-bottom: 2px;
	margin-left: 5px;
	margin-right: 5px;
}
QFrame .HeaderButton::menu-indicator 
{
    subcontrol-origin: padding;
	subcontrol-position: bottom right;
	position: relative;
    left: -2px;
}
QFrame .HeaderButton::menu-indicator:pressed, QFrame .HeaderButton::menu-indicator:open
{
	position: relative;
	top: 1px;
}
QFrame .HeaderButton:hover 
{
	background: qradialgradient(cx: 0.3, cy: -0.4, fx: 0.3, fy: -0.4, radius: 1.35, stop: 0 #fff, stop: 1 #bbb);
}
QFrame .HeaderButton:pressed 
{
	background: qradialgradient(cx: 0.4, cy: -0.1, fx: 0.4, fy: -0.1, radius: 1.35, stop: 0 #fff, stop: 1 #ddd);
}</string>
